{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "ES2022",
	  "lib": [
		"ES2022"
	  ],
	  "outDir": "dist",
	  "rootDir": "src",
	  "sourceMap": true,
	  "strict": true,
	  "esModuleInterop": true,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "skipLibCheck": true
	},
	"exclude": [
	  "node_modules",
	  ".vscode-test"
	]
  }
  